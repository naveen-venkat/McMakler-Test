{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Neo": {
    "dataSource": "MongoDBDS",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "hazardous": true,
          "fastest": true,
          "bestByYear": true,
          "bestByMonth": true
        }
      }
    }
  }
}
